{
  "name": "chaltteok_fe",
  "version": "1.1.2",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "pod-install": "cd ios && bundle exec pod install",
    "android:prod:clean": "cd android && ./gradlew clean && cd .. && npm run android:prod",
    "android:dev:clean": "cd android && ./gradlew clean && cd .. && npm run android:dev",
    "android:dev": "APP_ENV=development NODE_ENV=development EAS_NO_VCS=1 npx expo run:android",
    "android:prod": "APP_ENV=production NODE_ENV=production EAS_NO_VCS=1 npx expo run:android",
    "android:build:release": "cd android && ./gradlew assembleRelease bundleRelease",
    "android:install:release": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "android:run:release": "npx expo run:android --variant release",
    "android:build:release:prod": "cd android && APP_ENV=production NODE_ENV=production ./gradlew assembleRelease bundleRelease",
    "android:run:dev": "npx expo run:android --variant debug",
    "metro:prod": "APP_ENV=production NODE_ENV=production npx expo start --dev-client --no-dev --minify --clear",
    "ios:prod": "APP_ENV=production NODE_ENV=production EAS_NO_VCS=1 npx expo run:ios",
    "build:android:prod": "APP_ENV=production NODE_ENV=production cd android && ./gradlew bundleRelease",
    "build:android:dev": "NODE_ENV=development cd android && ./gradlew bundleRelease",
    "android:release:local": "APP_ENV=production NODE_ENV=production EAS_NO_VCS=1 npx expo run:android --variant release",
    "android:playstore:test": "npm run build:android:prod && npm run android:build:release:prod && npm run android:install:release",
    "android:build:release:local": "APP_ENV=production NODE_ENV=production EAS_NO_VCS=1 npx expo run:android --variant release --no-build-cache",
    "deploy:android": "APP_ENV=production NODE_ENV=production EAS_NO_VCS=1 npx expo run:android --variant release --no-build-cache",
    "version:bump": "npm version patch && echo '버전이 업데이트되었습니다. Android 버전도 수동으로 업데이트해주세요.'",
    "debug:prod": "APP_ENV=production NODE_ENV=production npx expo start --dev-client --clear",
    "test:network": "APP_ENV=production NODE_ENV=production npx expo run:android --variant release && adb logcat | grep -E '(Axios|Network|Error)'"
  },
